  <div id="overlay"></div>
  <script>
    const root = document.documentElement;
    const marqueeElementsDisplayed = Number(getComputedStyle(root).getPropertyValue("--marquee-elements-displayed"));
    const marqueeContent = document.querySelector("ul.marquee-content");

    root.style.setProperty("--marquee-elements", marqueeContent.children.length.toString());

    for (let i = 0; i < marqueeElementsDisplayed; i++) {
      marqueeContent.appendChild(marqueeContent.children[i].cloneNode(true));
    }
  </script>
  <script>
    // Get all the necessary elements
    const category = document.getElementById("category");
    const bxug = document.getElementById("bxug");
    const bxii = document.getElementById("bxii");
    const bxmv = document.getElementById("bxmv");
    const ug = document.getElementById("ug");
    const ii = document.getElementById("ii");
    const mv = document.getElementById("mv");
    const backButtons = document.querySelectorAll("#back");

    // Function to hide category and show the clicked div
    function showDiv(divToShow) {
      category.classList.add("hidden");
      divToShow.classList.remove("hidden");
    }

    // Function to show category and hide all other divs
    function showCategory() {
      category.classList.remove("hidden");
      [ug, ii, mv].forEach((div) => div.classList.add("hidden"));
    }

    // Add click event listeners to the boxes
    bxug.addEventListener("click", () => showDiv(ug));
    bxii.addEventListener("click", () => showDiv(ii));
    bxmv.addEventListener("click", () => showDiv(mv));

    // Add click event listeners to all back buttons
    backButtons.forEach((button) => {
      button.addEventListener("click", showCategory);
    });
  </script>

  <script>
    // Get all images with id of "zoom"
    document.querySelectorAll("#zoom").forEach((img) => {
      img.addEventListener("click", function () {
        const overlay = document.getElementById("overlay");
        overlay.innerHTML = ""; // Clear the overlay content

        // Create a new image element
        const zoomedImg = new Image();
        zoomedImg.src = this.src; // Use the source of the clicked image

        // Remove any Tailwind aspect classes like aspect-w-* or aspect-h-*
        const clonedClassList = [...this.classList].filter((className) => !className.startsWith("aspect-"));
        zoomedImg.classList.add(...clonedClassList); // Add back non-aspect classes

        zoomedImg.style.cursor = "zoom-out"; // Change cursor to indicate zoom-out

        overlay.appendChild(zoomedImg);
        overlay.classList.add("show"); // Show the overlay

        // Add click event to close the zoomed image and hide the overlay
        overlay.addEventListener("click", function () {
          overlay.classList.remove("show");
        });
      });
    });
  </script>

